<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
   <children>
      <Pane prefHeight="400.0" prefWidth="600.0">
         <children>
            <TextField fx:id="txtEntrada" layoutX="34.0" layoutY="68.0" prefHeight="25.0" prefWidth="532.0" promptText="Entrada" />
            <Button fx:id="botaoTraduzir" layoutX="267.0" layoutY="121.0" mnemonicParsing="false" onAction="#acaoDoBotao" text="Traduzir" textOverrun="LEADING_ELLIPSIS" />
            <Label fx:id="labelSaida" alignment="CENTER" contentDisplay="CENTER" layoutX="9.0" layoutY="176.0" prefHeight="201.0" prefWidth="581.0" textAlignment="CENTER" />
         </children>
         <cursor>
            <Cursor fx:constant="OPEN_HAND" />
         </cursor>
      </Pane>
   </children>
</GridPane>
